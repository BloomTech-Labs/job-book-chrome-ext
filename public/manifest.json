{
  "name": "Job Book",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "A Chrome extension that allows users to save job URL's with a user-facing dashboard that displays saved URL's. Users can login and register from both the front-end and the extension.",
  "content_scripts": [{
    "js": [],
    "matches": [],
    "css": []
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": {},
    "default_icon": {
      "16": "",
      "32": "",
      "48": "",
      "128": ""
    }
  },
  "icons": {
    "16": "",
    "32": "",
    "48": "",
    "128": ""
  },
  "permissions": ["contextMenus", "tabs", "storage", "activeTab"],
  "content_security_policy": "script-src 'self' 'sha256-ILo8Sg5ameOh7EJPaZmIQYqDkAozAPY56yCIVu/8hPo='; object-src 'self'",
  "web_accessible_resources" : ["*.html", "*.png", "*.js"]
}
